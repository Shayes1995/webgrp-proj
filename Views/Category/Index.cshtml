@model CategoryIndexVm;

<h1>Categories</h1>

<p>
    @Html.ActionLink("Create new category", "Create")
</p>


<table>
    <thead>
        <tr>
            <th>Category name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var category in Model.Categories)
        {
            <tr>
                <td>@category.Name</td>
            </tr>
        }
    </tbody>
</table>